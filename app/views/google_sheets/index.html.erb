<div class="container">
  <h1>Google sheets</h1>
  <ul class="list-group">
    <% @google_sheets.each do |table| %>
      <li class="list-group-item list-group-item-action">
        <div class="row">
          <div class="col=12 col-md-2">
            <a href="https://docs.google.com/spreadsheets/d/<%= table.value %>" target="_blank"><%= table.var %></a>
          </div>
          <div class="col=12 col-md-10">
            <%= table.description %>
          </div>
        </div>
      </li>
    <% end %>
  </ul>
</div>
